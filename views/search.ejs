<div class="author">
    <h1 class="author__heading">Search Results</h1>
</div>

<sectipn class="a">
    <ul class="articles-ul">
        <% data.forEach(post=> { %>
            <li>
                <a href="/post/<%= post._id %>">
                    <span>
                        <%= post.title %>
                    </span>
                    <span class="articles-list_date">
                        <%= post.createdAt.toDateString() %>
                    </span>
                </a>
            </li>
            <% }); %>
    </ul>
</sectipn>